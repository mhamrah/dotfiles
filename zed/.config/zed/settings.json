{
  "vim_mode": true,
  "minimap": {
    "show": "auto"
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "lsp_document_colors": "background",
  "agent": {
    "default_profile": "write",
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "ollama",
      "model": "gpt-oss:latest"
    }
  },
  "format_on_save": "on",
  "formatter": "language_server",
  "autosave": "on_focus_change",
  "base_keymap": "VSCode",
  "buffer_font_family": "CommitMono Nerd Font",
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "theme": {
    "mode": "system",
    "light": "Subliminal Nightfall Cloudy",
    "dark": "Subliminal Nightfall Cloudy"
  },
  "context_servers": {
    "cloudflare-web": {
      "enabled": false,
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://browser.mcp.cloudflare.com/sse"
      ]
    },
    "cloudflare-builds": {
      "enabled": false,
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://builds.mcp.cloudflare.com/sse"
      ]
    },
    "cloudflare-docs": {
      "enabled": false,
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://docs.mcp.cloudflare.com/sse"
      ],
      "env": {}
    },
    "cloudflare-workers": {
      "enabled": true,
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://bindings.mcp.cloudflare.com/sse"
      ],
      "env": {}
    }
  }
}
